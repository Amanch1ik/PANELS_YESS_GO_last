# Example environment file for Admin Panel (copy to admin-panel/.env before building)

# API base used by the built frontend (do not include extra path segments)
# For development use a proxy (e.g. /api), for production set the real API host.
VITE_API_BASE_URL=/api
VITE_DEV_MODE=false
VITE_DIRECT_API=false
VITE_APP_BASE=/

# Optional monitoring
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_SENTRY_DSN=https://xxx@sentry.io/xxx



